{
  "name": "@tanstack/ai-opentelemetry",
  "version": "0.0.3",
  "description": "OpenTelemetry instrumentation for TanStack AI",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanStack/ai.git",
    "directory": "packages/typescript/ai-opentelemetry"
  },
  "keywords": [
    "ai",
    "opentelemetry",
    "tracing",
    "observability",
    "tanstack",
    "instrumentation"
