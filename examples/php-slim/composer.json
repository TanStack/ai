{
  "name": "tanstack/ai-php-example",
  "description": "PHP Slim Framework example for TanStack AI",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "../../packages/php/tanstack-ai"
    }
  ],
  "require": {
    "php": ">=8.5.0",
    "slim/slim": "^4.15.1",
    "slim/psr7": "^1.8.0",
    "vlucas/phpdotenv": "^5.6.2",
    "monolog/monolog": "^3.9.0",
    "anthropic-ai/sdk": "^0.4.0",
    "openai-php/client": "^0.18.0",
    "tanstack/ai": "@dev",
    "symfony/http-client": "^7.4.1"
  },
  "require-dev": {
    "slim/psr7": "^1.8.0"
  },
  "autoload": {
    "psr-4": {
      "TanStack\\AI\\Example\\": "src/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8000 -t public public/index.php",
    "start-anthropic": "php -S 0.0.0.0:8000 -t public public/anthropic-server.php",
    "start-openai": "php -S 0.0.0.0:8000 -t public public/openai-server.php"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
