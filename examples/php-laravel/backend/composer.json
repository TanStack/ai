{
  "name": "tanstack/ai-laravel-example",
  "description": "Laravel example for TanStack AI",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "../../../packages/php/tanstack-ai"
    }
  ],
  "require": {
    "php": ">=8.2",
    "laravel/framework": "^11.0",
    "anthropic-ai/sdk": "^0.3.0",
    "openai-php/client": "^0.10.0",
    "tanstack/ai": "@dev"
  },
  "require-dev": {
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "scripts": {
    "start": "php artisan serve --host=0.0.0.0 --port=8020"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
